---
import BaseLayout from './BaseLayout.astro';

const title = 'Learning Journal | Index Notes';
const description = `This is where I log notes for everything that I've read, watched and listened to.`;
const permalink = 'https://learning.chiubaca.com/index-notes';
---
<BaseLayout title={title} description={description} permalink={permalink}>
  <nav><a href="/">/🏠Home</a> / 🔢 index-notes </nav>
  <header> 
    <h1>🔢 Index Notes</h1>
    <p>Catalog of high level topics. Click through to see references to notes related to this topic</p>
    <hr />
  </header>
  
  <slot/>
</BaseLayout>
